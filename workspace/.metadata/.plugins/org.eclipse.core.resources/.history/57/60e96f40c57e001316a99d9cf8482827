package se.legarre.ecommerce.model;

public final class Category
{
	private String id;
	private String desc;
	private String root_category;

	public Category(String id, String desc)
	{
		this.id = id;
		this.desc = desc;
	}

	public String getId()
	{
		return id;
	}

	public String getDesc()
	{
		return desc;
	}

}
